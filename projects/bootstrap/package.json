{
  "name": "bootstrap-classic",
  "version": "0.3.2",
  "description": "Example webpack project for compiling HTML stats pages",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=development webpack serve",
    "staging": "NODE_ENV=staging webpack build && npm run lint:html",
    "build": "NODE_ENV=production webpack build && npm run lint:html",
    "test": "vitest",
    "format": "dprint check",
    "lint": "npm-run-all --aggregate-output --parallel lint:styles lint:js lint:spelling lint:md",
    "lint:js": "eslint --fix-dry-run --cache --cache-location .cache/.eslintcache .",
    "lint:styles": "stylelint \"src/**/*.{css,scss,html}\" --cache --cache-location .cache/.stylelintcache",
    "lint:html": "node scripts/nu-html-checker.mjs",
    "lint:md": "markdownlint-cli2 \"**/*.md\" \"!node_modules\"",
    "lint:spelling": "cspell lint --show-context  \"**/*.{html,md}\""
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27",
    "@types/bootstrap": "^5.2",
    "@vralle/nu-html-checker": "^0.2",
    "@vralle/tool-configs": "^0.3",
    "autoprefixer": "^10.4",
    "babel-loader": "^10.0",
    "css-loader": "^7.1",
    "css-minimizer-webpack-plugin": "^7.0",
    "glob": "^11.0",
    "html-bundler-webpack-plugin": "^4.20",
    "image-minimizer-webpack-plugin": "^4.1",
    "markdownlint-cli2": "^0.17",
    "mini-svg-data-uri": "^1.4",
    "npm-run-all2": "^8.0",
    "postcss": "^8.5",
    "postcss-loader": "^8.1",
    "sass-embedded": "^1.87",
    "sass-loader": "^16.0",
    "sharp": "^0.34",
    "stylelint-config-twbs-bootstrap": "^16.0",
    "terser-webpack-plugin": "^5.3",
    "vitest": "^3.1",
    "webpack": "^5.99",
    "webpack-cli": "^6.0",
    "webpack-dev-server": "^5.2"
  },
  "dependencies": {
    "@popperjs/core": "^2.11",
    "bootstrap": "^5.3"
  }
}
